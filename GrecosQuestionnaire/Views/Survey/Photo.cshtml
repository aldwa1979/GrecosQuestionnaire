@model GrecosQuestionnaire.Models.Photo

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Resolution</th>
                <th>Size</th>
            </tr>
        </thead>
        <tbody>

                <tr>
                    <td>
                        @Model.Id
                    </td>
                    <td>
                        @Model.Name
                    </td>
                    <td>
                        @Model.Resolution
                    </td>
                    <td>
                        @Model.Size
                    </td>
                    
                </tr>
        </tbody>
    </table>