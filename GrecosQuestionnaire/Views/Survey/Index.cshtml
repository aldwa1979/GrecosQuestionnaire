@using System.Globalization
@using GrecosQuestionnaire.Models
@model IEnumerable<GrecosQuestionnaire.Models.Question>

@{
    Layout = "_LayoutSurvey";
}


@{
    ViewBag.Title = "Ankieta";
}


<div class="col-md-3"></div>

@{
    int page = int.Parse(ViewData["page"].ToString());
    int hotel = ViewBag.Hotel;
    int numberOfQuestion = 0;
    int? dana = ViewBag.Number;

#nullable enable
    List<string?> danaSubClass = ViewBag.NumberSubClass;
}

<br />
<br />
<br />
<br />
<br />

<div class="container">
    @Html.ValidationSummary()
</div>

<div class="container ">
    <div class="row justify-content-md-center">
        <div class="col-md-8" style="margin-top: 40px; ">

            @using (Html.BeginForm("Index", "Survey", FormMethod.Post, new { role = "form", @style = "color: #005EA8" }))
