                <div style="text-align: center; color:black; font-weight: 700">Step @ViewBag.Page / 11</div>
                <br />

                <div style="text-align: center">
                    @if (page == 1)
                    {

                    }
                    else
                    {
                        <button type="submit" class="btn btn-primary" style="background-color: #1b6ec2" name="action" value="back">
                            Back
                            <span class="glyphicon glyphicon-chevron-left"></span>
                        </button>
                    }

                    @if (page == 11)
                    {
                        <button type="submit" class="btn btn-primary alert-danger" name="action" value="save">
                            Save and Exit
                            <span class="glyphicon glyphicon-chevron-right"></span>
                        </button>
                    }

                    else
                    {
                        <button type="submit" class="btn btn-primary" style="background-color: #1b6ec2" name="action" value="next">
                            Next Page
                            <span class="glyphicon glyphicon-chevron-right"></span>
                        </button>

                        <button type="submit" class="btn btn-primary alert-danger" name="action" value="save">
                            Save and Exit
                            <span class="glyphicon glyphicon-chevron-right"></span>
                        </button>
                    }

                    <br />
                </div>

