                            string flipValue = "flip" + numberOfQuestion;
                            string panelValue = "panel" + numberOfQuestion;
                        }

                        <div id="@flipValue" style="background-color: #1b6ec2">

                            @Html.Label(question.Title, question.Title, new { @class = "control-label", @style = "font-weight: bold ;color: white; margin-bottom: 0; font-size: 1.1em;" })
                            <span class="glyphicon glyphicon-chevron-down" style="color: white"></span>
                        </div>

                        <div id="@panelValue" style="background-color: #b5dbff">
                            @if (!string.IsNullOrEmpty(question.Subtitle))
                            {
                                <br />
                                @Html.Label(question.Subtitle, question.Subtitle, new { @class = "control-label small", @style = "color: gray; margin-top: -5px; margin-bottom: 0;" })
