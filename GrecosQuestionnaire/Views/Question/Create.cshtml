@model GrecosQuestionnaire.Models.QuestionModel

@{
    ViewBag.Title = "Adding a question";
}

    <h2>Adding a question</h2>
@Html.ActionLink("Return to the question list", "Index", "Question")

@using (Html.BeginForm("Create", "Question", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <hr />
    @Html.ValidationSummary(true)
    @Html.Partial("_Create", Model)
}
